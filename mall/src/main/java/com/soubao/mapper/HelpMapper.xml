<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.soubao.dao.HelpMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.soubao.entity.Help">
        <id column="help_id" property="helpId" />
        <result column="help_sort" property="helpSort" />
        <result column="help_title" property="helpTitle" />
        <result column="help_info" property="helpInfo" />
        <result column="help_url" property="helpUrl" />
        <result column="add_time" property="addTime" />
        <result column="type_id" property="typeId" />
        <result column="page_show" property="pageShow" />
        <result column="is_show" property="isShow" />
        <result column="keywords" property="keywords" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        help_id, help_sort, help_title, help_info, help_url, add_time, type_id, page_show, is_show, keywords
    </sql>

</mapper>
